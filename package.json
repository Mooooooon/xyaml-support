{
  "name": "xyaml-support",
  "displayName": "XYAML Support",
  "description": "支持一种XML+YAML混合格式",
  "icon": "images/icon.jpeg",
  "version": "0.3.0",
  "publisher": "Simamiemie",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mooooooon/xyaml-support.git"
  },
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Programming Languages",
    "Formatters"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/node": "20.x",
    "typescript": "^5.4.5"
  },
  "contributes": {
    "languages": [{
      "id": "xyaml",
      "aliases": ["XYAML", "xyaml"],
      "extensions": [".xyaml"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "xyaml",
      "scopeName": "source.xyaml",
      "path": "./syntaxes/xyaml.tmLanguage.json"
    }]
  }
}
